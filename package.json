{
  "name": "dtimer",
  "version": "0.3.2",
  "description": "Distributed timer backed by Redis.",
  "main": "index.js",
  "scripts": {
    "test": "DEBUG=dtimer istanbul cover _mocha",
    "posttest": "eslint lib test; istanbul check-coverage",
    "lint": "eslint lib test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/enobufs/dtimer.git"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "debug": "^3.1.0",
    "ioredis": "^3.2.2",
    "lured": "^1.0.1",
    "underscore": "^1.8.3",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "async": "^2.6.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.5",
    "sinon": "^4.5.0"
  },
  "keywords": [
    "timer",
    "timed",
    "scheduler",
    "event",
    "cluster",
    "redis"
  ],
  "author": "enobufs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enobufs/dtimer/issues"
  },
  "homepage": "https://github.com/enobufs/dtimer",
  "eslintConfig": {
    "rules": {
      "callback-return": [
        2,
        [
          "callback",
          "cb",
          "next",
          "done"
        ]
      ],
      "camelcase": [
        2,
        {
          "properties": "never"
        }
      ],
      "comma-dangle": 0,
      "comma-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "indent": [
        2,
        4,
        {
          "SwitchCase": 1
        }
      ],
      "keyword-spacing": [
        "error",
        {
            "before": true,
            "after": true
        }
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "max-len": [
        1,
        120,
        4,
        {
          "ignoreComments": true,
          "ignoreUrls": true
        }
      ],
      "no-console": 0,
      "no-extra-boolean-cast": [
        0
      ],
      "no-new": 2,
      "no-spaced-func": [
        2
      ],
      "no-trailing-spaces": [
        2
      ],
      "no-unused-vars": [
        2,
        {
          "args": "all"
        }
      ],
      "no-use-before-define": [
        2,
        "nofunc"
      ],
      "semi": [
        2,
        "always"
      ],
      "space-before-function-paren": [
        2,
        {
          "anonymous": "always",
          "named": "never"
        }
      ]
    },
    "env": {
      "node": true,
      "mocha": true
    },
    "extends": "eslint:recommended"
  }
}
